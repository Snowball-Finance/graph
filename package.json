{
  "name": "snowgraph",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "create": "graph create  --node {{host}}:8020/ snow-graph",
    "deploy": "graph deploy  --node {{host}}:8020/ snow-graph",

    "create-dev": "graph create   --node {{host}}/:8020/ snow-graph",
    "deploy-dev": "graph deploy   --node {{host}}/:8020/ snow-graph",
    "create-local": "graph create --node http://localhost:8020/  snow-graph",
    "remove-local": "graph remove --node http://localhost:8020/  snow-graph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001  snow-graph"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.25.1",
    "@graphprotocol/graph-ts": "0.24.1"
  }
}
